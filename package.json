{
  "name": "token-io",
  "version": "1.2.10",
  "description": "",
  "main": "dist/token-io.js",
  "scripts": {
    "test": "npm run testNode",
    "testBrowser": "./node_modules/karma/bin/karma start test/karma.config.js",
    "testBrowserDev": "API_ENV=dev ./node_modules/karma/bin/karma start test/karma.config.js",
    "testBrowserStg": "API_ENV=stg ./node_modules/karma/bin/karma start test/karma.config.js",
    "testNode": "node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 5000 --watch --colors \"test/**/*.spec.js\"",
    "testNodeDev": "API_ENV=dev node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 8000 --watch --colors \"test/**/*.spec.js\"",
    "testNodeStg": "API_ENV=stg node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 8000 --watch --colors \"test/**/*.spec.js\"",
    "bambooDev": "API_ENV=dev node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 8000 --colors -R mocha-bamboo-reporter  \"test/**/*.spec.js\"",
    "bambooStg": "API_ENV=stg node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 8000 --colors -R mocha-bamboo-reporter  \"test/**/*.spec.js\"",
    "build": "npm run buildBrowser && npm run buildNode && WEBPACK_ENV=build npm run buildBrowser && WEBPACK_ENV=build npm run buildNode",
    "buildBrowser": "API_ENV=stg webpack --config config/webpack.config.js",
    "buildBrowserw": "API_ENV=stg webpack --config config/webpack.config.js --progress --colors -w",
    "buildNode": "API_ENV=stg webpack --config config/webpack.config.node.js",
    "buildNodew": "API_ENV=stg webpack --config config/webpack.config.node.js --progress --colors -w",
    "prepublish": "npm run build"
  },
